{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "requireConfigFile": false
  },
  "extends": ["eslint:recommended", "google", "plugin:react-hooks/recommended"],
  "env": {
    "browser": true
  },
  "rules": {
    "guard-for-in": "off",
    "no-redeclare": "off",
    "indent": ["warn", 4],
    "max-len": ["warn", 120],
    "require-jsdoc": "off"
  },
  "globals": {
    "JSX": true,
    "console": true
  }
}
